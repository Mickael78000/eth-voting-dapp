{
  "rewrites": [
    {
      "source": "/contracts/(.*)",
      "destination": "/build/contracts/$1"
    },
    {
      "source": "/css/(.*)",
      "destination": "/src/css/$1"
    },
    {
      "source": "/js/(.*)",
      "destination": "/src/js/$1"
    },
    {
      "source": "/fonts/(.*)",
      "destination": "/src/fonts/$1"
    },
    {
      "source": "/",
      "destination": "/src/index.html"
    }
  ]
}